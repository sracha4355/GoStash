{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug current Go test file",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${fileDirname}",
      "args": [
        "${fileBasename}"
      ],
      "env": {
        "GOFLAGS": "-count=1"
      },
      "showLog": true
    }
  ]
}
